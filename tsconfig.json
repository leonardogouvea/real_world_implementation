{
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2017",                                  
    "module": "commonjs",    
    "allowJs": true,                            
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": false,                                     
    "skipLibCheck": true  ,
                                  
  },
  "include": [
    "src/**/*.ts", "src/database/models/user.model.js"  ],
    "entities": ["/**/*.entity.{ts,js}"],
  "exclude": ["node_modules"]
}
